incoming                = /var/www/repos/ros-shadow-fixed/ubuntu/queue/all
run_dinstall            = 0
post_upload_command     = ssh rosbuild@repos.rog.org -- /usr/bin/reprepro -b /var/www/repos/ros-shadow-fixed/ubuntu --ignore=emptyfilenamepart -V processincoming all

